<p><%= link_to "Home", "/" %> | <%= link_to "Teams Index", "/teams" %> | <%= link_to "Members Index", "/members" %></p>
<%= link_to "Injured Members", "/members?injured=true" %>
<h1><%= @header %></h1>


<% @members.each do |member| %>
  <h3><%= link_to member.name, "/members/#{member.id}" %></h3>
  <p>Wage: <%= member.wage %></p>
  <p>Injured? <%= member.injured %></p>
  <p>Team: <%= member.team.name %></p>
<% end %>
